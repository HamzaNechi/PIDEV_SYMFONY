{% extends 'base-back.html.twig' %}


{% block title %}Ajouter qualifying{% endblock %}

{% block body %}
<div class="page-body">
          <div class="container-fluid">
            <div class="page-header">
              <div class="row">
              </div>
            </div>
          </div>
          <!-- Container-fluid starts-->
          <div class="container-fluid">
            <div class="row">
            <div class="col-sm-12 col-xl-2"></div>
              <div class="col-sm-12 col-xl-8">
                <div class="row">
                  
                  <div class="col-sm-12">
                    <div class="card">
                      <div class="card-header pb-0">
                        <h5>Ajouter qualifying</h5>
                      </div>
                      <div class="card-body">
                        
                        {{ form_start(form,{'attr':{'novalidate':'novalidate' } } ) }}
                          <div class="mb-3 row">
                            <label class="col-sm-3 col-form-label" >Q1</label>
                            <div class="col-sm-9">
                              {{ form_errors(form.q1)}}
                              {{ form_widget(form.q1)}}
                            </div>
                          </div>

                          <div class="mb-3 row">
                            <label class="col-sm-3 col-form-label" >Q2</label>
                            <div class="col-sm-9">
                            {{ form_errors(form.q2)}}
                              {{ form_widget(form.q2)}}
                            </div>
                          </div>
                          
                          <div class="mb-3 row">
                            <label class="col-sm-3 col-form-label" >Q3</label>
                            <div class="col-sm-9">
                            {{ form_errors(form.q3)}}
                              {{ form_widget(form.q3)}}
                            </div>
                          </div>
                          <div class="mb-3 row">
                            <label class="col-sm-3 col-form-label" >Position</label>
                            <div class="col-sm-9">
                              {{ form_widget(form.position)}}
                            </div>
                          </div>
                      </div>
                      <div class="card-footer pt-0">
                        {{ form_widget(form.Ajouter)}}
                        {{ form_widget(form.Annuler)}}
                      </div>
                      
                      {{ form_end(form)}}
                    </div>
                  </div>
                </div>
              </div>
              
            </div>
          </div>
          <!-- Container-fluid Ends-->
        </div>
{% endblock %}